{
  "name": "tabletop-tracker",
  "version": "0.1.0",
  "description": "Track board game plays, wins, losses and more!",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "db:reset": "npx supabase db reset",
    "db:push": "npx supabase db push",
    "db:diff": "npx supabase db diff",
    "types:generate": "npx supabase gen types typescript --local > app/lib/database.types.ts",
    "types:generate:remote": "npx supabase gen types typescript --project-id $PROJECT_REF --schema public > app/lib/database.types.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "next": "^14.0.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
